            <mn>2</mn>
          </mrow>
        </msup>
        <mrow class="MJX-TeXAtom-ORD">
          <mo stretchy="false">|</mo>
        </mrow>
        <msup>
          <mi>h</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>3</mn>
          </mrow>
        </msup>
        <mo stretchy="false">)</mo>
        <mo>=</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mn>1</mn>
            <mrow>
              <mi>Z</mi>
              <mo stretchy="false">(</mo>
              <mi>&#x03C8;<!-- ψ --></mi>
              <mo>,</mo>
              <msup>
                <mi>h</mi>
                <mrow class="MJX-TeXAtom-ORD">
                  <mn>3</mn>
                </mrow>
              </msup>
              <mo stretchy="false">)</mo>
            </mrow>
          </mfrac>
        </mrow>
